{
  "expo": {
    "name": "Dispopen",
    "slug": "Dispopen",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "./assets/frames/*" 
    ],
    "ios": {
      "bundleIdentifier": "com.company.dispopen",
      "associatedDomains": [
        "applinks:www.dispopen.com"
      ],
      "config": {
        "usesNonExemptEncryption": false
      },
      "supportsTablet": true
    },
    "android": {
      "package": "com.company.dispopen",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/CabinetGrotesk-Black.otf", 
        "./assets/fonts/CabinetGrotesk-Bold.otf",
      "./assets/fonts/CabinetGrotesk-Extrabold.otf",
    "./assets/fonts/CabinetGrotesk-Medium.otf",
  "./assets/fonts/CabinetGrotesk-Regular.otf"]
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos to let you share them with your friends.",
          "cameraPermission": "The app accesses your camera to let you take photos to share with your friends."
        }
      ],
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
